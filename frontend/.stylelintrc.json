{
	"extends": [
		"stylelint-config-standard-scss", "stylelint-config-recommended-vue/scss"
	],
	"plugins": [
		"stylelint-prettier"
	],
	"overrides": [
		{
		  "files": [
				"**/*.scss", "**/*.vue"
			]
		}
	],
	"rules": {
		"prettier/prettier": true,
		"max-nesting-depth": 4,
		"at-rule-no-unknown": null
	}
}